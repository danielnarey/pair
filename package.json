  "author": "Daniel C. Narey",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.3.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-types": "^1.0.2",
    "microbundle": "^0.12.0-next.6",
    "np": "^5.0.3",
    "updtr": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
