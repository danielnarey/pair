{
  "name": "@danielnarey/pair",
  "version": "0.0.0",
  "description": "A tiny functional data structure for working with paired values",
  "source": "index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.modern.js",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "build": "microbundle",
    "lint": "eslint --fix index.js src",
    "release": "npm run build && np",
    "sync": "git pull && git add . && git commit -m 'automated sync' && git push",
    "test": "npm run lint && ava",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielnarey/pair"
  },
  "keywords": [
    "pair",
    "immutable",
    "functional",
    "cons",
    "tuple",
    "two"
